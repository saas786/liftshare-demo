{
  "name": "liftshare",
  "version": "0.0.0",
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.vue$": "vue3-jest",
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "jest tests"
  },
  "dependencies": {
    "@fawmi/vue-google-maps": "^0.9.4",
    "mosha-vue-toastify": "^1.0.23",
    "vue": "^3.2.21",
    "vue3-date-time-picker": "^2.3.4"
  },
  "devDependencies": {
    "@types/google.maps": "^3.46.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/babel-preset-app": "^4.5.15",
    "@vue/test-utils": "^2.0.0-rc.16",
    "autoprefixer": "^10.4.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "miragejs": "^0.1.42",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0",
    "vue3-jest": "^27.0.0-alpha.1"
  }
}
